{
    "name": "palpalani/baylinks-laravel",
    "description": "PHP/Laravel framework SDK for BayLinks.",
    "license": "MIT",
    "keywords": [
        "palpalani",
        "laravel",
        "baylinks-laravel"
    ],
    "authors": [
        {
            "name": "palPalani",
            "email": "palani.p@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Prasanth",
            "email": "prasanth.s@targetbay.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/palpalani/baylinks-laravel",
    "require": {
        "php": "^8.3",
        "illuminate/contracts": "^10.0 || ^11.0 || ^12.0",
        "saloonphp/saloon": "^3.14",
        "spatie/laravel-package-tools": "^1.92.7"
    },
    "require-dev": {
        "larastan/larastan": "^3.8.0",
        "laravel/pint": "^1.25",
        "nunomaduro/collision": "^8.8.3",
        "orchestra/testbench": "^10.6.0",
        "pestphp/pest": "^4.1",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0.0",
        "pestphp/pest-plugin-type-coverage": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0.3",
        "phpstan/phpstan-phpunit": "^2.0.7"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "PalPalani\\BayLinks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PalPalani\\BayLinks\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "aliases": {
                "BayLinks": "PalPalani\\BayLinks\\Facades\\BayLinks"
            },
            "providers": [
                "PalPalani\\BayLinks\\BayLinksServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint --parallel",
        "test": "vendor/bin/pest --no-coverage",
        "test:arch": "pest --filter=arch",
        "test:type-coverage": "pest --type-coverage --min=100",
        "test-coverage": "vendor/bin/pest --coverage"
    }
}
